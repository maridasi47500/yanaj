
        <main class="site-main" role="main">
                <div class="page-content agenda agenda">
        <div class="page-banner">
            <div class="banner yellow-to-white">
    
    <div class="container relative banner-container">
        <div class="flex row flex-col lg:flex-row">
            <div class="flex">
                <div class="logo-overlay"></div>
                <h1 class="page-title">Agenda - page <%=@page%></h1>
            </div>

            <div class="pro-content-container flex flex-row items-center">
                                    <img class="character hidden lg:block ml-auto" alt=""
                        src="">
                
                            </div>
        </div>
    </div>
    </div>
</div>

        <div class="after-banner">
            <div class="container">
                <div class="flex row flex-col">
                    <div class="breadcrumb"><span><span><a href="https://www.yana-j.fr/">Accueil</a> &gt; <span class="breadcrumb_last" aria-current="page">Agenda - page <%=@page%></span></span></span></div>
                </div>
            </div>
        </div>

        <div class="archive-agenda">
            <div class="container">
<%@i=0%>
            <%@events.group_by{|x|x[0]}.to_a.each do |date, events|%>
                <div class="flex row items-center justify-between flex-wrap">
                                        <h3 class="event-year text-black-200 font-bold mb-2"><%=date.capitalize%></h3>
                                                            <%if @i == 0%>

             <%=render "events/pagesnav"%>
                       
                    <%end%>
                                    </div>

                <div class="agenda-list cards-container flex row flex-col md:flex-row items-stretch justify-between flex-wrap">
                                                                                                                                    <div class="agenda-card-container flex flex-col md:flex-row justify-between w-full lg:w-1/2 px-4">
    <%=render partial: "agendaitem", collection: events.map{|y|y[1]}.flatten, as: :event%>
                   </div>
<%@i+=1%>
<%end%>
                <div class="flex row items-center justify-between pb-10">
                                        <h3 class="event-year text-black-200 font-bold"></h3>
                           <%=render "events/pagesnav"%>

                                    </div>
            </div>
        </div>
    </div>
        </main>